    {
      "courseId": "5237777",
      "courseName": "Cloud Computing",
      "assessmentDate": "2025-30-01",
      "questions": [
        {
          "id": 1,
          "question": "What is Cloud Computing?",
          "options": [
            "A way to store and access data over the internet",
            "A type of weather forecasting",
            "A type of computer virus",
            "A hardware device for data storage"
          ],
          "answer": "A way to store and access data over the internet"
        },
        {
          "id": 2,
          "question": "Which of the following is NOT a type of cloud deployment model?",
          "options": [
            "Public Cloud",
            "Private Cloud",
            "Hybrid Cloud",
            "Legacy Cloud"
          ],
          "answer": "Legacy Cloud"
        },
        {
          "id": 3,
          "question": "What does 'IaaS' stand for in cloud computing?",
          "options": [
            "Infrastructure as a Service",
            "Internet as a Solution",
            "Information as a Service",
            "Integration as a System"
          ],
          "answer": "Infrastructure as a Service"
        },
        {
          "id": 4,
          "question": "Which of these is a primary benefit of cloud computing?",
          "options": [
            "Fixed infrastructure costs",
            "Limited accessibility",
            "Scalability on demand",
            "Inflexibility in resource allocation"
          ],
          "answer": "Scalability on demand"
        },
        {
          "id": 5,
          "question": "Which cloud service model focuses on providing a platform for developers to build applications?",
          "options": [
            "IaaS",
            "PaaS",
            "SaaS",
            "DaaS"
          ],
          "answer": "PaaS"
        },
        {
          "id": 6,
          "question": "Which of the following is an example of a SaaS application?",
          "options": [
            "Google Docs",
            "Microsoft Azure",
            "Amazon EC2",
            "Docker"
          ],
          "answer": "Google Docs"
        },
        {
          "id": 7,
          "question": "What is the primary goal of cloud load balancing?",
          "options": [
            "To enhance security",
            "To distribute workloads evenly across resources",
            "To reduce bandwidth usage",
            "To minimize data storage costs"
          ],
          "answer": "To distribute workloads evenly across resources"
        },
        {
          "id": 8,
          "question": "Which of the following is a key characteristic of cloud computing?",
          "options": [
            "Virtualization",
            "Limited connectivity",
            "Physical hardware dependency",
            "Static infrastructure"
          ],
          "answer": "Virtualization"
        },
        {
          "id": 9,
          "question": "What does 'Auto-scaling' mean in cloud computing?",
          "options": [
            "Manually increasing resources for an application",
            "Automatically adjusting resources based on demand",
            "Scheduling resource allocation in advance",
            "Reducing resources permanently"
          ],
          "answer": "Automatically adjusting resources based on demand"
        },
        {
          "id": 10,
          "question": "Which of the following best describes 'Serverless Computing'?",
          "options": [
            "A type of cloud computing without servers",
            "Cloud providers manage server resources dynamically",
            "Applications hosted locally without a server",
            "Running applications without internet"
          ],
          "answer": "Cloud providers manage server resources dynamically"
        }
      ],
      "id": "687d"
    },
    {
      "courseId": "8241111",
      "courseName": "Business Analytics",
      "assessmentDate": "2025-31-01",
      "questions": [
        {
          "id": 1,
          "question": "What is Business Analytics?",
          "options": [
            "A process of analyzing business data to make decisions",
            "A type of business accounting",
            "Developing business strategies without data",
            "A method for predicting weather patterns"
          ],
          "answer": "A process of analyzing business data to make decisions"
        },
        {
          "id": 2,
          "question": "Which of the following is a key component of Business Analytics?",
          "options": [
            "Data Analysis",
            "Hardware Setup",
            "Manual Data Entry",
            "Data Destruction"
          ],
          "answer": "Data Analysis"
        },
        {
          "id": 3,
          "question": "What is the primary goal of Business Analytics?",
          "options": [
            "To improve decision-making",
            "To eliminate the use of data",
            "To increase manual reporting",
            "To reduce the use of technology"
          ],
          "answer": "To improve decision-making"
        },
        {
          "id": 4,
          "question": "Which of these is a common tool used in Business Analytics?",
          "options": [
            "Tableau",
            "Photoshop",
            "CorelDRAW",
            "Premiere Pro"
          ],
          "answer": "Tableau"
        },
        {
          "id": 5,
          "question": "What is 'Predictive Analytics' in the context of Business Analytics?",
          "options": [
            "A method to predict future trends using data",
            "A technique for creating business presentations",
            "A manual way to organize data",
            "A form of digital marketing"
          ],
          "answer": "A method to predict future trends using data"
        },
        {
          "id": 6,
          "question": "Which of the following is an example of descriptive analytics?",
          "options": [
            "Summarizing historical sales data",
            "Predicting future customer behavior",
            "Optimizing supply chain operations",
            "Developing machine learning models"
          ],
          "answer": "Summarizing historical sales data"
        },
        {
          "id": 7,
          "question": "What does a 'Dashboard' provide in Business Analytics?",
          "options": [
            "A graphical representation of key metrics",
            "A detailed written report",
            "An alternative to data storage",
            "A manual method to record data"
          ],
          "answer": "A graphical representation of key metrics"
        },
        {
          "id": 8,
          "question": "Which statistical technique is commonly used in Business Analytics?",
          "options": [
            "Regression Analysis",
            "Cryptography",
            "Photosynthesis",
            "Gene Editing"
          ],
          "answer": "Regression Analysis"
        },
        {
          "id": 9,
          "question": "What is the role of data visualization in Business Analytics?",
          "options": [
            "To make data insights more understandable",
            "To hide complex data",
            "To replace statistical analysis",
            "To store data permanently"
          ],
          "answer": "To make data insights more understandable"
        },
        {
          "id": 10,
          "question": "Which of the following describes 'Prescriptive Analytics'?",
          "options": [
            "Recommending actions based on data insights",
            "Summarizing past data",
            "Storing data securely",
            "Predicting data loss"
          ],
          "answer": "Recommending actions based on data insights"
        }
      ],
      "id": "b593"
    }
