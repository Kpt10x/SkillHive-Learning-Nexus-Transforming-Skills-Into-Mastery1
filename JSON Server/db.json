          "id": 3,
          "question": "What does 'IaaS' stand for in cloud computing?",
          "options": [
            "Infrastructure as a Service",
            "Internet as a Solution",
            "Information as a Service",
            "Integration as a System"
          ],
          "answer": "Infrastructure as a Service"
